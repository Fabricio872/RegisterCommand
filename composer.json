{
    "name": "fabricio872/register-command",
    "description": "Symfony bundle for registering new users from terminal",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["library", "symfony", "symfony-bundle", "console", "cli", "select", "checkbox", "radio"],
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/annotations": "^1.13|^2.0",
        "doctrine/orm": "^2.9",
        "symfony/config": "^4.4|^5|^6.0",
        "symfony/console": "^4.4|^5|^6.0",
        "symfony/dependency-injection": "^4.4|^5|^6.0",
        "symfony/http-kernel": "^4.4|^5|^6.0",
        "symfony/security-bundle": "^4.4|^5|^6.0",
        "symfony/serializer": "^4.4|^5|^6.0",
        "symfony/validator": "^4.4|^5|^6.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-doctrine": "^1.3",
        "rector/rector": "^0.15.16",
        "symfony/phpunit-bridge": "^4.4|^5|^6.0",
        "symplify/easy-coding-standard": "^11.2.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "@ecs",
            "@rector",
            "@phpstan"
        ],
        "cs-fix": [
            "@ecs-fix",
            "@rector-fix"
        ],
        "ecs": "vendor/bin/ecs check",
        "ecs-fix": "vendor/bin/ecs check --fix",
        "rector": "vendor/bin/rector process --dry-run",
        "rector-fix": "vendor/bin/rector process",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}
