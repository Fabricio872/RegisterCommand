{
    "name": "fabricio872/register-command",
    "description": "Symfony bundle for registering new users from terminal",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/config": "^4.4|^5",
        "symfony/dependency-injection": "^4.4|^5",
        "symfony/http-kernel": "^4.4|^5",
        "symfony/security-bundle": "^4.4|^5",
        "symfony/serializer": "^4.4|^5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\Tests\\": "tests/"
        }
    }
}
