{
    "name": "fabricio872/register-command",
    "description": "Symfony bundle for registering new users from terminal",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["library", "symfony", "symfony-bundle", "console", "cli", "select", "checkbox", "radio"],
    "authors": [
        {
            "name": "Fabricio",
            "email": "fabricio872@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/annotations": "^1.13",
        "doctrine/orm": "^2.9",
        "illuminate/support": "^8.78",
        "symfony/config": "^5.3|^6.0",
        "symfony/console": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "symfony/http-kernel": "^5.3|^6.0",
        "symfony/security-bundle": "^5.3|^6.0",
        "symfony/serializer": "^5.3|^6.0",
        "symfony/validator": "^5.3|^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3|^6.0",
        "symfony/test-pack": "^1.0",
        "symplify/easy-coding-standard": "^9.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabricio872\\RegisterCommand\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-psr": "vendor/bin/ecs check src",
        "fix-psr": "vendor/bin/ecs check src --fix"
    }
}
